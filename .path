# Defaults
export PATH="~/.bin:/usr/local/sbin:/usr/local/bin:/usr/local/opt:/usr/bin:/usr/sbin:/bin:/sbin:/opt/X11/bin:/usr/X11/bin"

# npm
export PATH="~/node_modules/.bin:/usr/local/share/npm/bin:/usr/local/lib/node_modules:$PATH"

# GNU Core Utils
#export PATH="$(brew --prefix coreutils)/libexec/gnubin:$PATH"

# homebrew-php PEAR scripts
#export PATH="$(brew --prefix php56)/bin:$PATH"

# Make rvm happy
export PATH="$PATH:$HOME/.rvm/bin"
